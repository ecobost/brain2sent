import numpy as np
import h5py

# Torch features (https://github.com/deepmind/torch-hdf5/blob/master/doc/usage.md)
feats_file = h5py.File('feats.h5', 'r')
feats = feats_file['feats']
feats = np.array(feats)


# Matlab (v 7.3) to python
stim_file = h5py.File('Stimuli.mat','r') 
data = stim_file.get('data/variable1') 
data = np.array(data) # For converting to numpy array




